package TestNg;


import java.util.concurrent.TimeUnit;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.Test;

public class google {

	//webdriver should be put in main class to access every in program
	WebDriver driver = new ChromeDriver();

	@BeforeMethod     	//Setup driver ,maximize window i.e steps before creating script
	public void setUp(){
	
		System.setProperty("webdriver.chrome.driver", "C:\\chromedriver_win32\\chromedriver.exe");
		driver.manage().window().maximize();
		driver.manage().deleteAllCookies();
		driver.manage().timeouts().implicitlyWait(20,TimeUnit.SECONDS);
		driver.get("https:\\)www.google.com");
			
		
	}
	
	@Test //main program should be write inside test
	
	
	public void testData() {
		
	
		String title=driver.getTitle();
		
	}
	
	
	
	@AfterMethod //after test finishing our test to close test write inside aftermethod
	public void finish() {
		
	driver.quit();	
		
		
	}
	
	
	
	
	
}
