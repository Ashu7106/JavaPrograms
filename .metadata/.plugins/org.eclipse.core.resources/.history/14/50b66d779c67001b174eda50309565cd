package JavaBasics;

public class PatternInjava2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		
		for(int i=1;i<=5;i++) {
			
		
		for(int j=5;j>=i;j--) {
			
			
		System.out.print(" ");	
			
		}
			
	    System.out.println();		
			
			}
		
		
	for(int p=3;p<=5;p++){
		
	for(int q=3;q<=p;q++) {
		
	System.out.print("*");	
		
		}
		
		System.out.println();
		
	  }
		
		
		
		
	}

}
